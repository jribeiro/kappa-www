{
  "name": "kappa-www",
  "version": "2.3.0",
  "description": "web interface for kappa proxied private npm registry",
  "main": "plugin/index.js",
  "bin": "tasks/clean.js",
  "scripts": {
    "postinstall": "grunt build",
    "test": "npm run jscs && npm run lint && tape test/*.js",
    "jscs": "./node_modules/.bin/jscs .",
    "lint": "jshint -c .jshintrc plugin/*.js public/javascripts/*.js"
  },
  "keywords": [
    "npm",
    "registry",
    "private",
    "proxy",
    "kappa"
  ],
  "engines": {
    "node": ">=0.10.30"
  },
  "engineStrict": true,
  "dependencies": {
    "async": "^0.9.0",
    "bootstrap": "^3.3.2",
    "bunyan": "^1.3.3",
    "github-url-from-git": "^1.4.0",
    "grunt-cli": "^0.1.13",
    "handlebars": "^2.0.0",
    "jquery": "^2.1.3",
    "marked": "^0.3.3",
    "minimist": "^1.1.0",
    "nedb": "^1.1.1",
    "npm": "^2.4.1",
    "npm-registry-client": "^6.0.7",
    "npmconf": "^2.1.1",
    "rimraf": "^2.2.8",
    "typeahead.js": "^0.10.5",
    "underscore": "^1.7.0"
  },
  "peerDependencies": {
    "kappa": ">=1.0.0-rc.8"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.3.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-contrib-requirejs": "^0.4.4",
    "grunt-contrib-watch": "^0.6.1",
    "hapi": "^8.1.0",
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "kappa": "^1.0.0-rc.8",
    "tape": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samsel/kappa-www.git"
  },
  "author": "Sam Selvanathan",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/samsel/kappa-www/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/samsel/kappa-www/issues"
  },
  "homepage": "https://github.com/samsel/kappa-www"
}
