{
	"directory": {
		"asset": "public",
		"template": "templates",
		"cache": ".cache"

	},
	"layoutFile": "layout",
	"syncInterval": 3600,
	"page": {
		"url": "/-/page/",
		"maxResults": 10
	},	
	"search": {
		"url": "/-/search/",
		"maxResults": 15,
		"enable": true
	},
	"registry": {
		"timeout": 1000,
		"staleOk": true,
		"dumpURL": "-/all"
	},
	"defaultDomain": "github.com",
	"nedb": {
		"name": "local.npm.db"
	}
}